<h1>Listing dashboard_articles</h1>
<%= link_to 'Articles', articles_path %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.body %></td>
        <td><%= link_to 'Show', dashboard_article_path(article) %></td>
        <td><%= link_to 'Edit', edit_dashboard_article_path(article) %></td>
        <td><%= link_to 'Destroy', dashboard_article_path(article), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article', new_dashboard_article_path %>
